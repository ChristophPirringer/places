<h1>Our List of Places!!</h1>

<p>Your Places:</p>
<ul>
  <% @places.each() do |place| %>
  <li><%= place.name() %></li>
  <% end %>
</ul>

<form action="/success" method="post">
  <label for="name">Where have you been?</label>
  <input type="text" name="name" id="name" class="form-control">
  <!-- <br> -->
  <a href="/fresh_start" class="btn btn-warning" id="fresh_start">Start Fresh</a>
  <button type="submit" class="btn btn-success">Save</button>

</form>
